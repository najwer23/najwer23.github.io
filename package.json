    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "chart.js": "^4.4.9",
    "gh-pages": "^6.3.0",
    "najwer23snacks": "^4.0.14",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-chartjs-2": "5.3.0",
