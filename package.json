    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "chart.js": "^4.4.7",
    "gh-pages": "^6.2.0",
    "najwer23snacks": "^4.0.4",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "^19.0.0",
