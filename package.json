{
  "name": "najwer23.github.io",
  "version": "5.0.0",
  "keywords": [
    "react",
    "gh-pages",
    "typescript",
    "vite",
    "vitest",
    "css modules",
    "front-end",
    "yarn"
  ],
  "homepage": "https://najwer23.github.io/",
  "author": "Mariusz Najwer",
  "type": "module",
  "scripts": {
    "bench": "vitest bench",
    "build": "tsc --build --clean && vite build",
    "deploy": "yarn run build && gh-pages -d build && rimraf build",
    "format": "prettier --write \"**/*.{json,mdx,ts,tsx,md,css,mjs}\"",
    "god": "ncu -u && yarn install && yarn run start",
    "preview": "yarn build && vite preview --open --host",
    "sort": "npx sort-package-json",
    "start": "vite --open --host"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.55.0",
    "@types/chart.js": "^2.9.41",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "chart.js": "^4.4.4",
    "gh-pages": "^6.1.1",
    "najwer23snacks": "^2.3.8",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-chartjs-2": "5.2.0",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.26.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3"
  },
  "packageManager": "yarn@4.4.1"
}
